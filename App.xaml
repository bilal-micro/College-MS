<Application x:Class="CollegeMS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CollegeMS"
             xmlns:VMPage="clr-namespace:CollegeMS.ViewModel">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="/Styles/DataGridStyles.xaml"/>
                <ResourceDictionary Source="/Styles/AbdoStyles.xaml"/>

            </ResourceDictionary.MergedDictionaries>


            <VMPage:DoctorViewModel x:Key="doctorViewModel" />
            <VMPage:LoginViewModel x:Key="loginViewModel" />
            <VMPage:MainWindowViewModel x:Key="mainWindowViewModel"  />
            <VMPage:ManagerViewModel x:Key="managerViewModel"  />
            <VMPage:StaffViewModel x:Key="staffViewModel"  />
            <VMPage:StudentViewModel x:Key="studentViewModel" />
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
